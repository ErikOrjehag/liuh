<launch>

    <node pkg="nodelet"
          type="nodelet"
          args="manager"
          name="camera_nodelet_manager"
          output="screen" />

    <node pkg="nodelet"
          type="nodelet"
          args="load liuh_camera/CameraNodelet camera_nodelet_manager"
          name="liuh_camera"
          output="screen">
          <param name="fps_top" value="0" />
          <param name="fps_bottom" value="10" />
    </node>
</launch>
